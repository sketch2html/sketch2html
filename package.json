    "logs": "skpm log -f"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.13.0",
    "jszip": "^3.1.5",
    "lodash": "^4.17.11",
    "skpm": "^1.1.8"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.10",
    "blanket": "^1.2.3",
    "coveralls": "^3.0.2",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "author": "army8735 <army8735@qq.com>"
}
