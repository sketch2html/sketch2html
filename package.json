{
  "name": "sketch2code",
  "version": "0.0.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch2code",
    "manifest": "src/manifest.json",
    "main": "sketch2code.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
